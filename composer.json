{
    "name": "websolutionsz/laravel-disk-monitor",
    "description": "Laravel Disk Monitor",
    "keywords": [
        "websolutionsz",
        "laravel-disk-monitor"
    ],
    "homepage": "https://github.com/websolutionsz/laravel-disk-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Mansi Prajapati",
            "email": "mansi@conceptualize.ae",
            "homepage": "https://github.com/websolutionsz/laravel-disk-monitor",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Websolutionsz\\DiskMonitor\\": "src",
            "Websolutionsz\\DiskMonitor\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Websolutionsz\\DiskMonitor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Websolutionsz\\DiskMonitor\\DiskMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
